{
    "manifest_version": 3,
    "name": "HTML to CSV Exporter",
    "version": "1.0",
    "description": "Exports HTML content as CSV",
    "permissions": [
        "activeTab",
        "scripting",
        "downloads",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}